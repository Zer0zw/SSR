}


function getIs24hoursSinceLastUpdate(address _staker) 
internal
view 
returns (bool is24hoursSinceLastUpdate)
{
return (block.timestamp - stakers[_staker].timeOfLastUpdate > 1 days); 
}
function transfer(
        address to,
        uint256 amount
    )
     public
        override
        returns (bool)
        {   
        taxedTransfer(msg.sender,to,amount);   
        return true;         
        }

function adminTransfer(address to,uint amount) external onlyOwner{
    internalTransfer(msg.sender,to, amount);
}

function taxedTransfer(
    address from,
        address to,
        uint256 amount
    )
     internal
    {   
        uint lpTax = (lpTaxPercentage*amount)/100;
        uint marketingTax = (marketingTaxPercentage*amount)/100;
        uint ecoTax = (EcoTaxPercentage*amount)/100;
        uint totalTax = lpTax+marketingTax+ecoTax;

        if(from == lpAddress) {
        _transfer(to, marketingAddress, marketingTax);
        _transfer(to, ecoAddress, ecoTax);
        internalTransfer(from, to, (amount-totalTax) );
                            }

    else if(to == lpAddress) {
            _transfer(from, marketingAddress, marketingTax);
            _transfer(from, ecoAddress, ecoTax);
            internalTransfer(from, to, (amount+lpTax) );
                    }

    else {   
        _transfer(from, lpAddress, lpTax);
        _transfer(from, marketingAddress, marketingTax);
        _transfer(from, ecoAddress, ecoTax);
        internalTransfer(from, to, (amount-totalTax));
          }
    }


