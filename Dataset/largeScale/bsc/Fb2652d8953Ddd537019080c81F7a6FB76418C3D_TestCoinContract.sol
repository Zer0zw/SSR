    constructor() ERC20("TestCoin", "TC") {
         _mint(msg.sender, 450000000*10**decimals());
    }
    function _mint(address _account, uint256 _amount) internal override{
        require(totalSupply()+_amount <= maxSupply, "Exceeding Max Supply of Tokens");
        super._mint(_account, _amount);
    }
    // If address has no Staker struct, initiate one. If address already was a stake,
    // calculate the rewards and add them to unclaimedRewards, reset the last time of
    // deposit and then add _amount to the already staked amount.
    // Burns the amount staked.
    /// Stakes and Adds unclaimed rewards to total (if any)
    function stake(uint256 _amount) external nonReentrant {
        require(isStakingStarted,"Staking not yet started");
        require(
            balanceOf(msg.sender) >= _amount,
            "Can't stake more than you own"
        );
        require(totalSupply()+_amount <= maxSupply, "Cannot Stake Once Max Supply Limit is Reached");
        
        if (stakers[msg.sender].staked == 0) {
            stakers[msg.sender].staked = _amount;
            stakers[msg.sender].timeOfLastUpdate = block.timestamp;
            stakers[msg.sender].unclaimedRewards = 0;
        } else {
            uint256 rewards = calculateRewards(msg.sender);
            stakers[msg.sender].unclaimedRewards += rewards;
            stakers[msg.sender].staked += _amount;
            stakers[msg.sender].timeOfLastUpdate = block.timestamp;
        }
        totalStaked += _amount;
        _burn(msg.sender, _amount);
    }
    /// Withdraw all stake and rewards(if any)
    function withdrawAll() external nonReentrant {
        require(stakers[msg.sender].staked > 0, "You have no deposit");
        uint256 _rewards = calculateRewards(msg.sender) +
        stakers[msg.sender].unclaimedRewards;
        uint256 _deposit = stakers[msg.sender].staked;
        totalStaked -= _deposit;
        stakers[msg.sender].staked = 0;
        stakers[msg.sender].timeOfLastUpdate = 0;
        uint256 _amount = _rewards + _deposit;
        if( totalSupply() + _amount > maxSupply){
            _amount=maxSupply-totalSupply();
        }
        _mint(msg.sender, _amount);
    }
    /// @dev Function useful for fron-end that returns user stake and rewards by address
    /// Get User Stake And Rewards Info
    function getDepositInfo(address _user)
        public
        view
        returns (uint256 _stake, uint256 _rewards)
    {
        if(stakers[_user].staked == 0){
            return(0,0);
        }
        _stake = stakers[_user].staked;
        _rewards =
            calculateRewards(_user) +
            stakers[msg.sender].unclaimedRewards;
        return (_stake, _rewards);
    }
    /// @dev Calculate the rewards since the last update on Deposit info
    function calculateRewards(address _staker)
        internal
        view
        returns (uint256 rewards)
    {
        bool is24hoursPassed = getIs24hoursSinceLastUpdate(_staker);
        if(is24hoursPassed){
            uint rewardAmount = ((stakers[_staker].staked*dailyRewardsPool)/totalStaked);
            return rewardAmount;
        }
    }
