 function transferFrom(
 address from,
 address to,
 uint256 amount
    )
        public
        override
     returns (bool)
        {   
        _spendAllowance(from, msg.sender, amount);
        taxedTransfer(from,to,amount);
         return true;   
        }


        function internalTransfer(
        address from,
        address to,
        uint256 amount
    ) internal
        {   
        require(amount >= 100,"Amount (After Tax) Less Than 100 Base Units");
        require( to == lpAddress || balanceOf(to)+amount <= ((limitPerWalletPercentageDivByTen*totalSupply())/1000),"Cannot Exceed Limit Per Wallet");
        _transfer(from,to, amount);        
        }
