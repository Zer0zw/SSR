    function setEthPrice(uint newEthPrice) public onlyOwner {
        ethPrice = newEthPrice;
    }

    function setEthAddress(address newEthAddress) public onlyOwner {
        ethAddress = newEthAddress;
    }

    // Methods
    function withdrawFunds(address payable beneficiary, uint withdrawAmount) external onlyOwner {
        require(withdrawAmount <= address(this).balance, "Withdrawal exceeds limit");
        beneficiary.transfer(withdrawAmount);
    }

    function withdrawCustomTokenFunds(address beneficiary, uint withdrawAmount, address token) public onlyOwner {
        require(withdrawAmount <= IERC20(token).balanceOf(address(this)), "Withdrawal exceeds limit");
        IERC20(token).safeTransfer(beneficiary, withdrawAmount);
    }

    function withdrawEthFunds(address beneficiary, uint withdrawAmount) external onlyOwner {
        withdrawCustomTokenFunds(beneficiary, withdrawAmount, ethAddress);
    }

    function handlePayment(uint amountToMint) internal {
        uint amountSent = msg.value;
        if (amountSent == 0) {
            // ETH mint
            IERC20(ethAddress).safeTransferFrom(msg.sender, address(this), ethPrice * amountToMint);
        }
        else {
            // MATIC mint
            require(amountSent == maticPrice * amountToMint, "Received the wrong amount");
        }
    }
}

abstract contract PreSaleManager is Ownable {
    using SafeERC20 for IERC20;
    
    bool public preSaleLive;
    uint public maticPreSalePrice = 0.01 ether;
    uint public ethPreSalePrice = 0.01 ether;
    mapping(address => uint) public presaleAddresses;

    // Setter
    function togglePreSaleLive() public onlyOwner {
        preSaleLive = !preSaleLive;
    }

    function setPreSaleMaticPrice(uint newMaticPreSalePrice) public onlyOwner {
        maticPreSalePrice = newMaticPreSalePrice;
    }

    function setPreSaleEthPrice(uint newEthPreSalePrice) public onlyOwner {
        ethPreSalePrice = newEthPreSalePrice;
    }

    function setPresaleAddresses(address[] memory _presaleAddresses, uint amount) public onlyOwner {
        for (uint i = 0; i < _presaleAddresses.length; i++) {
            presaleAddresses[_presaleAddresses[i]] = amount;
        }
    }

    //Methods
    function handlePreSalePayment(uint amountToMint, address ethAddress) internal {
        uint amountSent = msg.value;
        if (amountSent == 0) {
            // ETH mint
            IERC20(ethAddress).safeTransferFrom(msg.sender, address(this), ethPreSalePrice * amountToMint);
        }
        else {
            // MATIC mint
            require(amountSent == maticPreSalePrice * amountToMint, "Received the wrong amount");
        }
    }
}

contract DiceWarNFT is ERC1155, ReentrancyGuard, Manager, PreSaleManager {
    using Strings for uint256;
    using ECDSA for bytes32;
    bytes32 public root = 0x6d9c535de70b6a3f24617b65e8da66a01718c0de02ff689d302219af0ca28429;
    uint public maxSupply = 900;
    uint public maxMintablePerTx = 5;
    uint public mintedTokens;    
    uint public maxPerWallet = 10;
    mapping(uint8 => mapping(uint256 => address)) public lvList; //tokenLevel => token id => address;
    mapping(uint => uint) public lvSum; // token level => token sum numbers

    mapping(address => uint) public mintedPerAddress;
    mapping(address => mapping(uint => uint[])) public holderTokenidList; // addr=>token Level=>token Id list array
        
    constructor () ERC1155("") { }

    // Setter
    function setMerkleRoot(string memory _newMerkleRoot) public onlyOwner {
        root = bytes32(bytes(_newMerkleRoot));
    }
    function setMaxSupply(uint newMaxSupply) public onlyOwner {
        maxSupply = newMaxSupply;
    }

    function setMaxPerWallet(uint newMaxPerWallet) public onlyOwner {
        maxPerWallet = newMaxPerWallet;
    }

    function setMaxMintablePerTx(uint newMaxMintablePerTx) public onlyOwner {
        maxMintablePerTx = newMaxMintablePerTx;
    }

    // Set URI
    function setUri(string memory newUri) public onlyOwner {
        _setURI(newUri);
    }

    // Methods
    function upgradeMint(address _addr, uint8 level) external admin {
        lvList[level+1][lvSum[level+1]] = _addr;
        holderTokenidList[_addr][level + 1].push(lvSum[level+1]);
        lvSum[level+1] ++;
        _mint(_addr, level + 1, 1, "");
    } 

    function airdrop(address[] memory addr, uint amount) public onlyOwner {
        require(amount > 0, "Amount less than 1");
        uint addrLength = addr.length;
        require(mintedTokens + (amount * addrLength) <= maxSupply, "No enough tokens left");

        uint i;
        for (i; i < addrLength; i++) {
            uint counter;
            while(counter < amount) {
                _mint(addr[i], 1, 1, "");
                lvList[1][lvSum[1]] = addr[i];
                holderTokenidList[addr[i]][1].push(lvSum[1]);
                lvSum[1] ++;
                mintedTokens++;
                counter++;
            }            
        }
    }

    function _whitelistVerify(bytes32[] memory _proof, bytes32 _root)
        internal
        view
        returns (bool)
    {
        return
            MerkleProof.verify(
                _proof,
                _root,
                keccak256(abi.encodePacked(msg.sender))
            );
    }
    
    function whitelistMint(uint amount, bytes32[] calldata _merkleProof) public payable nonReentrant {
        require(_whitelistVerify(_merkleProof, root), "Invalid merkle proof");
        require(preSaleLive, "Presale not live");
        require(amount > 0 && amount < maxMintablePerTx, "Amount not right");
        require(mintedTokens + amount <= maxSupply, "No enough tokens left");

        handlePreSalePayment(amount, ethAddress);

        uint counter;
        while(counter < amount) {
            _mint(msg.sender, mintedTokens + 1, 1, "");
            presaleAddresses[msg.sender]--;
            mintedPerAddress[msg.sender]++;
            holderTokenidList[msg.sender][1].push(lvSum[1]);
            lvList[1][lvSum[1]] = msg.sender;
            lvSum[1] ++;
            mintedTokens++;
            counter++;
        }
    }

    function mint(uint amount) public payable nonReentrant {
        require(saleLive, "Sale is not live");
        require(amount > 0 && amount <= maxMintablePerTx, "Not the right amount");
        require(mintedTokens + amount <= maxSupply, "No enough tokens left");

        handlePayment(amount);

        uint counter;
        while(counter < amount) {
            _mint(msg.sender, mintedTokens + 1, 1, "");
            mintedPerAddress[msg.sender]++;
            lvList[1][lvSum[1]] = msg.sender;
            holderTokenidList[msg.sender][1].push(lvSum[1]);
            lvSum[1]++;
            mintedTokens++;
            counter++;
        }
    }

    function burn(address account, uint8 tokenId, uint256 amount) external admin {
        _burn(account, tokenId, amount);
        uint listId = holderTokenidList[account][tokenId][holderTokenidList[account][tokenId].length - 1];
        lvList[tokenId][0] = lvList[tokenId][listId];
        lvList[tokenId][listId]= address(0);
        lvSum[tokenId] --;
        holderTokenidList[account][tokenId][0] = holderTokenidList[account][tokenId][holderTokenidList[account][tokenId].length - 1];
        holderTokenidList[account][tokenId].pop();
    }

    function balanceOfNft(address account, uint256 id ) external view returns(uint256){
        return balanceOf(account,id);
    }

    function getLvList (uint8 tokenId) external view  returns(address [] memory){
        address[] memory holderAddress = new address[](lvSum[tokenId]);
        for(uint i = 0; i < lvSum[tokenId] ; i ++){
            holderAddress[i] = lvList[tokenId][i];
        }
        return holderAddress;
    }

    function userMaxLevel(address account) external view returns (uint) {
        for (uint i = 4 ; i >0 ; i --){
            if(this.balanceOfNft(account, i) > 0) {
                return i;
            }
        }
        return 0;
    }

    /**
     * @dev See {IERC1155-safeTransferFrom}.
     */

    address[] public stakingAddress;
    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        require( tokenNotStaked(from, id) >= amount , "No enough unstaked token to transfer");
        holderTokenidList[to][id].push(holderTokenidList[from][id][holderTokenidList[from][id].length-1]);
        holderTokenidList[from][id].pop();
        _safeTransferFrom(from, to, id, amount, data);
    }

        /**
     * @dev See {IERC1155-safeBatchTransferFrom}.
     */
    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual override {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: transfer caller is not owner nor approved"
        );
        for(uint i = 0 ; i < ids.length ; i ++) {
            require( tokenNotStaked(from, ids[i]) >= amounts[0] , "No enough unstaked token to transfer");
            for(uint j = 0 ; j < amounts[i]; j ++){
                holderTokenidList[to][ids[i]].push(holderTokenidList[from][ids[i]][holderTokenidList[from][ids[i]].length-1]);
                holderTokenidList[from][ids[i]].pop();
            }
            
        }
        _safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    function tokenNotStaked(address userAddr, uint tokenLevel) public view returns(uint) {
        uint userStakeCount;
        for(uint i = 0; i < holderTokenidList[userAddr][tokenLevel].length ; i ++) {
            if(tokenStakedTimestamp[tokenLevel][holderTokenidList[userAddr][tokenLevel][i]] != 0){
                userStakeCount ++;
            }
        }
        uint number = this.balanceOfNft(userAddr, tokenLevel) - userStakeCount;
        return number;
    }

    mapping(uint => mapping(uint => uint)) public tokenStakedTimestamp; // token level => token id => StakedTimestamp
    event userStakeToken(address userAddr, uint tokenLevel);
    event userUnstakeAllToken(address userAddr);

    function stakeToken (uint tokenLevel) public nonReentrant returns(bool) {
        require(this.balanceOfNft(msg.sender, tokenLevel) > 0 , "Token not enough to stake");
        for(uint i = 0; i < holderTokenidList[msg.sender][tokenLevel].length ; i ++) {
            if(tokenStakedTimestamp[tokenLevel][holderTokenidList[msg.sender][tokenLevel][i]] == 0){
                tokenStakedTimestamp[tokenLevel][holderTokenidList[msg.sender][tokenLevel][i]] = block.timestamp;
                emit userStakeToken(msg.sender, tokenLevel);
                return true;
            }
        }
        return false;
    }

    function unstakeAllToken (address _userAddr) public  nonReentrant returns(bool) {
        require(_userAddr == msg.sender || addressAdmin[msg.sender] == true , "No auth to do this");
        userStakingRewards[_userAddr] = this.nftStakingRewards(_userAddr);
        for(uint j = 1 ; j < 5 ; j ++){
            for(uint i = 0; i < holderTokenidList[_userAddr][j].length ; i ++) {
                if(tokenStakedTimestamp[j][holderTokenidList[_userAddr][j][i]] != 0){
                    tokenStakedTimestamp[j][holderTokenidList[_userAddr][j][i]] = 0;
                }
            }
        }
        emit userUnstakeAllToken(_userAddr);
        return true;
    }

    function changeDayNftStakingRewards (uint _tokenLevel, uint _dailyRewards) public onlyOwner {
        require(_tokenLevel == 1 || _tokenLevel == 2 || _tokenLevel == 3 || _tokenLevel == 4,"level not in range" );
        dayNftStakingRewards[_tokenLevel] = _dailyRewards;
    }

    mapping(address => uint) public userStakingRewards;
    mapping(uint => uint) public dayNftStakingRewards;

    
    function nftStakingRewards (address _userAddr) external view returns(uint){
        uint stakingRewardsCount;
        for(uint tokenLevel=1 ; tokenLevel<5 ; tokenLevel ++){
            for(uint i = 0; i < holderTokenidList[_userAddr][tokenLevel].length ; i ++) {
                uint timeStamp = tokenStakedTimestamp[tokenLevel][holderTokenidList[_userAddr][tokenLevel][i]];
                if( timeStamp != 0){
                    uint dayGap = (block.timestamp - timeStamp) / 86400;
                    stakingRewardsCount += dayGap * dayNftStakingRewards[tokenLevel];
                }
            }
        }
        return stakingRewardsCount + userStakingRewards[_userAddr];

    }

    function claimAll(address _userAddr) external admin returns(bool) {
        userStakingRewards[_userAddr] = 0;
        return true;
    }

    function userNftListInfo (address _address, uint tokenLevel) public view returns(
        uint[] memory nftList,
        uint[] memory statusList
        ) {
        uint userLevelCount = holderTokenidList[_address][tokenLevel].length;
        uint[] memory lvStakingStatusList = new uint[](userLevelCount);
        uint[] memory userlevelList = new uint[](userLevelCount);
        for(uint i = 0; i < userLevelCount ; i ++){
            userlevelList[i] = holderTokenidList[_address][tokenLevel][i];
            lvStakingStatusList[i] = tokenStakedTimestamp[tokenLevel][holderTokenidList[_address][tokenLevel][i]];
        }
        return (userlevelList, lvStakingStatusList);
    }
}