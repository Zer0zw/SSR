    {
        uint256 newRewardPerToken = tokenRewardsPerTokenPoints[_token].sub(stakers[_user].lastTokenRewardPoints[_token]);
        uint256 rewards = getStakedUserValue(_user).mul(newRewardPerToken)
                                                .div(1e18)
