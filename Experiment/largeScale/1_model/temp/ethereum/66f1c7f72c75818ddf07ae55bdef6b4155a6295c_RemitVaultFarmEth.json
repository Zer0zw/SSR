{
    "Variables": {
        "User Stake Amount": [
            "depositedTokens"
        ],
        "User Stake Reward": [
            "totalEarnedTokens"
        ],
        "User Stake Time": [
            "depositTime"
        ],
        "Stake Token Address": [
            "trustedDepositTokenAddress"
        ],
        "Reward Token Address": [
            "trustedRewardTokenAddress"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "require(Token(trustedDepositTokenAddress).transferFrom(msg.sender, address(this), amountToDeposit)"
            }
        ],
        "getReward": [
            {
                "Function": "updateAccount",
                "Node": "require(Token(trustedRewardTokenAddress).transfer(account, pendingDivs))"
            },
            {
                "Function": "claim",
                "Node": "updateAccount(msg.sender)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "require(Token(trustedDepositTokenAddress).transfer(msg.sender, amountAfterFee)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "require(Token(trustedDepositTokenAddress).transfer(msg.sender, amountAfterFee))"
            }
        ]
    }
}