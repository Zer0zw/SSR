{
    "Variables": {
        "User Stake Amount": [
            "Stake.initialAmount"
        ],
        "User Stake Reward": [
            "Stake.finalAmount"
        ],
        "User Stake Time": [
            "Stake.timestamp"
        ],
        "Stake Token Address": [
            "TKN"
        ],
        "Reward Token Address": [
            "TKN"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "require(TKN.transferFrom(msg.sender, address(this), _amount))"
            }
        ],
        "getReward": [],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "require(TKN.transfer(msg.sender, _s.finalAmount))"
            }
        ]
    }
}