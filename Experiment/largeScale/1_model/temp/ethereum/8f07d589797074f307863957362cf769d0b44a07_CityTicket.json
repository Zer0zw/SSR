{
    "Variables": {
        "User Stake Amount": [
            "_amount"
        ],
        "User Stake Reward": [
            "what"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "city"
        ],
        "Reward Token Address": [
            "city"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "enter",
                "Node": "city.transferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [],
        "unStake": [
            {
                "Function": "leave",
                "Node": "city.transfer(msg.sender, what)"
            }
        ]
    }
}