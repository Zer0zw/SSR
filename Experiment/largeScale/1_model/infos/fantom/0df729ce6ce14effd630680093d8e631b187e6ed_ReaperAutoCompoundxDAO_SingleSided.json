{
    "Variables": {
        "User Stake Amount": [
            "pairBal"
        ],
        "User Stake Reward": [],
        "User Stake Time": [],
        "Stake Token Address": [
            "token"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IMasterChef(masterChef).deposit(poolId, pairBal)"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "IERC20(wftm).safeTransfer(msg.sender, callFeeToUser)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(token).safeTransfer(vault, pairBal.sub(withdrawFee))"
            }
        ]
    }
}