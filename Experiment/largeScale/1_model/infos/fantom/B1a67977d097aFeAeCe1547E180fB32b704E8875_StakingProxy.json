{
    "Variables": {
        "User Stake Amount": [
            "Claim.deposit"
        ],
        "User Stake Reward": [
            "Claim.gons"
        ],
        "User Stake Time": [
            "Claim.expiry"
        ],
        "Stake Token Address": [
            "HEC"
        ],
        "Reward Token Address": [
            "sHEC"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "IERC20(HEC).approve(staking, _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "claim",
                "Node": "IERC20(sHEC).transfer(_recipient, IsHEC(sHEC).balanceForGons(claimInfo.gons))"
            }
        ],
        "unStake": []
    }
}