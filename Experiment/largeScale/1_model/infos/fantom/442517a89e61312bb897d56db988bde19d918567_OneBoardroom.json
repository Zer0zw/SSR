{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "Memberseat.rewardEarned"
        ],
        "User Stake Time": [
            "timers"
        ],
        "Stake Token Address": [
            "share"
        ],
        "Reward Token Address": [
            "pegTokens"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "super.stake(amount)"
            }
        ],
        "getReward": [
            {
                "Function": "_claimReward",
                "Node": "IERC20(_token).safeTransfer(msg.sender, _reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "super.withdraw(amount)"
            }
        ]
    }
}