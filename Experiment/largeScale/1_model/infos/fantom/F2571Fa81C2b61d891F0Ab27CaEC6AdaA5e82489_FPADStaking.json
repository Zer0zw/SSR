{
    "Variables": {
        "User Stake Amount": [
            "User.deposits"
        ],
        "User Stake Reward": [
            "User.compounds",
            "User.total_ref_bonus"
        ],
        "User Stake Time": [
            "User.deposit_time"
        ],
        "Stake Token Address": [
            "FPADToken"
        ],
        "Reward Token Address": [
            "FPADToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "FPADToken.transferFrom(_userAddress, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "_claim_out",
                "Node": "FPADToken.transferFrom(address(frensVaultAddress), _addr, _amt)"
            },
            {
                "Function": "_claim_referral",
                "Node": "FPADToken.transferFrom(address(frensVaultAddress), _addr, _amt)"
            }
        ],
        "unStake": [
            {
                "Function": "_withdraw_principal",
                "Node": "FPADToken.transferFrom(address(this), _addr, _withdraw_amt)"
            }
        ]
    }
}