{
    "Variables": {
        "User Stake Amount": [
            "balanceOfPool"
        ],
        "User Stake Reward": [
            "rewardBalance"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "want"
        ],
        "Reward Token Address": [
            "output"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IStargateChef(masterchef).deposit(poolId, wantBal)"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "IERC20(wrapped).safeTransfer(caller, callFeeAmount)"
            },
            {
                "Function": "_harvest",
                "Node": "IERC20(wrapped).safeTransfer(caller, callFeeAmount)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(want).safeTransfer(vault, wantBal - withdrawalFeeAmount)"
            },
            {
                "Function": "retireStrat",
                "Node": "IERC20(want).transfer(vault, wantBal)"
            },
            {
                "Function": "panic",
                "Node": "IStargateChef(masterchef).emergencyWithdraw(poolId)"
            }
        ]
    }
}