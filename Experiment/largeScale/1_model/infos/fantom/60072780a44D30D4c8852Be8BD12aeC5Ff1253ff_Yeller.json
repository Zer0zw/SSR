{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt",
            "UserInfo.remainingYelTokenReward"
        ],
        "User Stake Time": [
            "PoolInfo.lastRewardTime"
        ],
        "Stake Token Address": [
            "PoolInfo.stakingToken"
        ],
        "Reward Token Address": [
            "yel"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.stakingToken.safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "deposit",
                "Node": "user.remainingYelTokenReward = safeRewardTransfer(msg.sender, pending)"
            },
            {
                "Function": "withdraw",
                "Node": "user.remainingYelTokenReward = safeRewardTransfer(tx.origin, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.stakingToken.safeTransfer(address(tx.origin), _amount)"
            }
        ]
    }
}