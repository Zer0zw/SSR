{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "tri"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(userAddress), address(this), _amount"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "safeTriTransfer(userAddress, pending"
            },
            {
                "Function": "withdraw",
                "Node": "safeTriTransfer(msg.sender, pending"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), user.amount"
            }
        ]
    }
}