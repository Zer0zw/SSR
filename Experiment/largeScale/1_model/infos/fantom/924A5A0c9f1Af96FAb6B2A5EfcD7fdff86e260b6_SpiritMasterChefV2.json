{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "lpToken"
        ],
        "Reward Token Address": [
            "SPIRIT"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "lpToken[pid].safeTransferFrom(msg.sender, address(this), amount"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "SPIRIT.safeTransfer(to, _pendingSpirit)"
            },
            {
                "Function": "withdrawAndHarvest",
                "Node": "SPIRIT.safeTransfer(to, _pendingSpirit)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "lpToken[pid].safeTransfer(to, amount)"
            },
            {
                "Function": "withdrawAndHarvest",
                "Node": "lpToken[pid].safeTransfer(to, amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "lpToken[pid].safeTransfer(to, amount)"
            }
        ]
    }
}