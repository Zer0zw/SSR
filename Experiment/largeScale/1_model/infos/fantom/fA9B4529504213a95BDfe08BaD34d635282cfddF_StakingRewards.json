{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": [
            "rewardTokens"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "IERC20(token).safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(msg.sender, amount)"
            },
            {
                "Function": "exit",
                "Node": "stakingToken.safeTransfer(msg.sender, amount)"
            }
        ]
    }
}