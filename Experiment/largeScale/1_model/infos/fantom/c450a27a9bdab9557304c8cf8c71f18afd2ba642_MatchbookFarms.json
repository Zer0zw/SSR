{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt",
            "UserInfo.rewardLockedUp"
        ],
        "User Stake Time": [
            "UserInfo.nextHarvestUntil"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "candle"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "payOrLockupPendingCandle",
                "Node": "safeCandleTransfer(msg.sender, totalRewards)"
            },
            {
                "Function": "harvestMany",
                "Node": "deposit(_pids[index], 0)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(msg.sender, _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(msg.sender, amount)"
            }
        ]
    }
}