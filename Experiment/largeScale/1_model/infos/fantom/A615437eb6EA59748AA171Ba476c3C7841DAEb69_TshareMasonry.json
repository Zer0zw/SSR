        ],
        "User Stake Reward": [],
        "User Stake Time": [
            "depositTime",
            "withdrawTime"
        ],
        "Stake Token Address": [
            "want"
        ],
        "Reward Token Address": [
            "tomb"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IMasonry(masonry).stake(wantBal)"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "IERC20(wrapped).safeTransfer(msg.sender, callFeeAmount)"
            },
            {
                "Function": "harvest",
                "Node": "IERC20(wrapped).safeTransfer(grimFeeRecipient, grimFeeAmount)"
            },
            {
                "Function": "harvest",
                "Node": "IERC20(wrapped).safeTransfer(insuranceFund, insuranceAmount)"
            },
            {
                "Function": "harvest",
                "Node": "IERC20(wrapped).safeTransfer(strategist, strategistFee)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(want).safeTransfer(vault, wantBal.sub(withdrawalFeeAmount))"
            },
            {
                "Function": "startUnlockWindow",
                "Node": "IMasonry(masonry).withdraw(IMasonry(masonry).balanceOf(address(this)))"
            },
            {
                "Function": "retireStrat",
                "Node": "IERC20(want).transfer(vault, wantBal)"
            },
            {
                "Function": "panic",
                "Node": "IMasonry(masonry).withdraw(balanceOfPool())"
            }
        ]
    }
