{
    "Variables": {
        "User Stake Amount": [
            "_balances"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "rewardsDuration"
        ],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": [
            "rewardsToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), id, amount, \"\""
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardsToken.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransferFrom(address(this), msg.sender, id, amount, \"\""
            }
        ]
    }
}