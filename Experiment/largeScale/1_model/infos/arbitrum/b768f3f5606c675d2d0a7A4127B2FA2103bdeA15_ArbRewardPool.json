{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "PoolInfo.token"
        ],
        "Reward Token Address": [
            "reward"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IERC20(pool.token).safeTransferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "deposit",
                "Node": "safeArbTransfer(msg.sender, pending)"
            },
            {
                "Function": "withdraw",
                "Node": "safeArbTransfer(msg.sender, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(pool.token).safeTransfer(msg.sender, _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "IERC20(pool.token).safeTransfer(msg.sender, _amount)"
            }
        ]
    }
}