{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "duration",
            "finishAt",
            "updatedAt"
        ],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": [
            "rewardsToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), realAmount)"
            },
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, feeCollector, amountDepositFee)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardsToken.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(msg.sender, _amount)"
            }
        ]
    }
}