            {
                "Function": "_deposit",
                "Node": "pool.want.safeTransferFrom(msg.sender, address(this), _wantAmt)"
            }
        ],
        "getReward": [],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.want.safeTransfer(_to, wantWithdrawn)"
            },
            {
                "Function": "_withdraw",
                "Node": "pool.want.safeTransfer(_to, wantWithdrawn)"
            },
            {
