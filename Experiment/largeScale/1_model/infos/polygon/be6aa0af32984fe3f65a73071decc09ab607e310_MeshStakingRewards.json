{
    "Variables": {
        "User Stake Amount": [
            "_balances"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "periodFinish"
        ],
        "Stake Token Address": [
            "ADMESH"
        ],
        "Reward Token Address": [
            "MESH"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), amount"
            },
            {
                "Function": "stakeFor",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), amount"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardsToken.safeTransfer(msg.sender, reward"
            },
            {
                "Function": "claim",
                "Node": "rewardsToken.safeTransfer(msg.sender, reward"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(msg.sender, amount"
            },
            {
                "Function": "withdrawAll",
                "Node": "stakingToken.safeTransfer(msg.sender, amount"
            }
        ]
    }
}