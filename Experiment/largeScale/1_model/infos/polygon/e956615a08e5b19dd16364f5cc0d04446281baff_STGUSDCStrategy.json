{
    "Variables": {
        "User Stake Amount": [
            "totalDeposits"
        ],
        "User Stake Reward": [
            "pendingReward",
            "contractBalance"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "depositToken"
        ],
        "Reward Token Address": [
            "rewardToken1"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "_mint(msg.sender, getSharesForDepositTokens(amount))"
            }
        ],
        "getReward": [
            {
                "Function": "reinvest",
                "Node": "require(rewardToken1.transfer(msg.sender, reinvestFee)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "require(depositToken.transfer(msg.sender, depositTokenAmount)"
            }
        ]
    }
}