{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount",
            "Stake.amount"
        ],
        "User Stake Reward": [
            "pendingReward"
        ],
        "User Stake Time": [
            "Stake.duration",
            "Stake.end"
        ],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": [
            "earnedToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "stakingToken.safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "claimReward",
                "Node": "earnedToken.safeTransfer(address(msg.sender), pending"
            },
            {
                "Function": "withdraw",
                "Node": "earnedToken.safeTransfer(address(msg.sender), pending"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(address(msg.sender), realAmount"
            }
        ]
    }
}