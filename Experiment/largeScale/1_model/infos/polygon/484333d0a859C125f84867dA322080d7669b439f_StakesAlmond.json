{
    "Variables": {
        "User Stake Amount": [
            "Record.amount"
        ],
        "User Stake Reward": [
            "Record.gain",
            "Record.gain2"
        ],
        "User Stake Time": [
            "Record.from",
            "Record.to"
        ],
        "Stake Token Address": [
            "asset"
        ],
        "Reward Token Address": [
            "asset2"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "start",
                "Node": "require(asset.transferFrom(msg.sender, address(this), _value))"
            }
        ],
        "getReward": [
            {
                "Function": "end",
                "Node": "require(asset.transferFrom(getOwner(), msg.sender, _interest))"
            },
            {
                "Function": "end",
                "Node": "require(asset2.transferFrom(getOwner(), msg.sender, _interest2))"
            }
        ],
        "unStake": [
            {
                "Function": "end",
                "Node": "require(asset.transfer(msg.sender, ledger[msg.sender][i].amount - _penalization))"
            },
            {
                "Function": "end",
                "Node": "require(asset.transfer(msg.sender, ledger[msg.sender][i].amount))"
            }
        ]
    }
}