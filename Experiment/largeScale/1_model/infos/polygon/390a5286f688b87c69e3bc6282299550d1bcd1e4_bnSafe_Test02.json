{
    "Variables": {
        "User Stake Amount": [
            "User.sM.totalStaked",
            "User.sT.totalStaked"
        ],
        "User Stake Reward": [
            "User.sM.unClaimedTokens",
            "User.sT.unClaimedTokens"
        ],
        "User Stake Time": [
            "User.sM.lastStakeTime",
            "User.sT.lastStakeTime"
        ],
        "Stake Token Address": [],
        "Reward Token Address": []
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stakeBNB",
                "Node": "user.sM.totalStaked = user.sM.totalStaked.add(msg.value)"
            },
            {
                "Function": "stakeToken",
                "Node": "user.sT.totalStaked = user.sT.totalStaked.add(tokenAmount)"
            }
        ],
        "getReward": [
            {
                "Function": "claimToken_M",
                "Node": "_mint(msg.sender, tokenAmount)"
            },
            {
                "Function": "claimToken_T",
                "Node": "_mint(msg.sender, tokenAmount)"
            }
        ],
        "unStake": [
            {
                "Function": "unStakeToken",
                "Node": "_transfer(address(this), msg.sender, tokenAmount)"
            }
        ]
    }
}