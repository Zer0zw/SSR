{
    "Variables": {
        "User Stake Amount": [
            "Claim.deposit"
        ],
        "User Stake Reward": [
            "Claim.gons"
        ],
        "User Stake Time": [
            "Claim.expiry"
        ],
        "Stake Token Address": [
            "DeFarm"
        ],
        "Reward Token Address": [
            "SOW"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "IERC20( SOW ).safeTransfer( warmupContract, _amount )"
            }
        ],
        "getReward": [
            {
                "Function": "claim",
                "Node": "IWarmup( warmupContract ).retrieve( _recipient, ISOW( SOW ).balanceForGons( info.gons ) )"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "IERC20( DeFarm ).safeTransfer( msg.sender, _amount )"
            },
            {
                "Function": "forfeit",
                "Node": "IERC20( DeFarm ).safeTransfer( msg.sender, info.deposit )"
            }
        ]
    }
}