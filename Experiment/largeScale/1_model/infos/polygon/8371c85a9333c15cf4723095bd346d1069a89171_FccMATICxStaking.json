{
    "Variables": {
        "User Stake Amount": [
            "User.totalStakedTokenUser",
            "Stake.amount"
        ],
        "User Stake Reward": [
            "Stake.reward",
            "Stake.harvestreward"
        ],
        "User Stake Time": [
            "Stake.staketime",
            "Stake.unstaketime"
        ],
        "Stake Token Address": [
            "stakeToken"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakeToken.transferFrom(msg.sender, address(this), amount1)"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "rewardToken.transfer(msg.sender, rewardTillNow)"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "stakeToken.transfer(msg.sender, stakersRecord[msg.sender][index].amount)"
            }
        ]
    }
}