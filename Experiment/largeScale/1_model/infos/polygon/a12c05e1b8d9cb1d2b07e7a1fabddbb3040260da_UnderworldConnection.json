{
    "Variables": {
        "User Stake Amount": [
            "tokensStakedByUser"
        ],
        "User Stake Reward": [
            "pointUser"
        ],
        "User Stake Time": [
            "StakeInfo.stakedAtBlock",
            "StakeInfo.lastHarvestBlock"
        ],
        "Stake Token Address": [
            "TokenExchange"
        ],
        "Reward Token Address": [
            "TokenReward"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "TokenExchange.transfer(msg.sender, 1 ether)"
            },
            {
                "Function": "stakeBatch",
                "Node": "TokenExchange.transfer(msg.sender, 1 ether)"
            }
        ],
        "getReward": [
            {
                "Function": "reward",
                "Node": "TokenReward.transfer(user, points)"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "IERC721(_collection).safeTransferFrom(address(this), _msgSender(), tokenId)"
            },
            {
                "Function": "unstakeBatch",
                "Node": "IERC721(_collection).safeTransferFrom(address(this), _msgSender(), tokenId)"
            }
        ]
    }
}