                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount.mul(userFeeStage[4]).div(100))"
            },
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount.mul(userFeeStage[5]).div(1000))"
            },
            {
