{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "DINO"
        ],
        "Reward Token Address": [
            "REWARD"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "transact",
                "Node": "DINO.safeTransferFrom(address(msg.sender), address(this), _amount"
            }
        ],
        "getReward": [
            {
                "Function": "transact",
                "Node": "safeRewardTransfer(msg.sender, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "transact",
                "Node": "DINO.safeTransfer(burnAddress, user.amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "DINO.safeTransfer(address(msg.sender), user.amount"
            }
        ]
    }
}