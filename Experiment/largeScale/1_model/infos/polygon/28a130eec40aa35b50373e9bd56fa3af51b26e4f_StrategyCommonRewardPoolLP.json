{
    "Variables": {
        "User Stake Amount": [
            "balanceOfWant",
            "balanceOfPool"
        ],
        "User Stake Reward": [
            "rewardsAvailable"
        ],
        "User Stake Time": [
            "lastHarvest"
        ],
        "Stake Token Address": [
            "want"
        ],
        "Reward Token Address": [
            "output"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IRewardPool(rewardPool).stake(wantBal)"
            }
        ],
        "getReward": [
            {
                "Function": "_harvest",
                "Node": "IRewardPool(rewardPool).getReward()"
            },
            {
                "Function": "harvest",
                "Node": "_harvest()"
            },
            {
                "Function": "managerHarvest",
                "Node": "_harvest()"
            },
            {
                "Function": "cowllectorHarvest",
                "Node": "_harvest()"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(want).safeTransfer(vault, wantBal.sub(withdrawalFeeAmount))"
            },
            {
                "Function": "retireStrat",
                "Node": "IERC20(want).transfer(vault, wantBal)"
            },
            {
                "Function": "panic",
                "Node": "IRewardPool(rewardPool).withdraw(balanceOfPool())"
            }
        ]
    }
}