{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.reward",
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "UserInfo.pendingAt"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "nsure"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount"
            },
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount"
            }
        ],
        "getReward": [
            {
                "Function": "claim",
                "Node": "safeNsureTransfer(msg.sender, pending)"
            },
            {
                "Function": "deposit",
                "Node": "safeNsureTransfer(msg.sender,pending)"
            },
            {
                "Function": "unstake",
                "Node": "safeNsureTransfer(msg.sender, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), amount"
            },
            {
                "Function": "unstake",
                "Node": "user.pendingWithdrawal = user.pendingWithdrawal.add(_amount)"
            }
        ]
    }
}