{
    "Variables": {
        "User Stake Amount": [
            "totalStaked",
            "vault.owner"
        ],
        "User Stake Reward": [
            "earned"
        ],
        "User Stake Time": [
            "vault.timestamp"
        ],
        "Stake Token Address": [
            "vaultInfo.nft"
        ],
        "Reward Token Address": [
            "vaultInfo.token"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "vaultid.nft.transferFrom(msg.sender, address(this), tokenId"
            }
        ],
        "getReward": [
            {
                "Function": "claim",
                "Node": "vaultid.token.mint(account, earned)"
            },
            {
                "Function": "claimForAddress",
                "Node": "vaultid.token.mint(account, earned)"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "vaultid.nft.transferFrom(address(this), account, tokenId)"
            },
            {
                "Function": "_unstakeMany",
                "Node": "vaultid.nft.transferFrom(address(this), account, tokenId)"
            }
        ]
    }
}