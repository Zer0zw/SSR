{
    "Variables": {
        "User Stake Amount": [
            "wantLockedTotal",
            "sharesTotal"
        ],
        "User Stake Reward": [
            "earnedAmt"
        ],
        "User Stake Time": [
            "lastEarnBlock"
        ],
        "Stake Token Address": [
            "wantAddress"
        ],
        "Reward Token Address": [
            "earnedAddress"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IERC20(wantAddress).safeTransferFrom(address(msg.sender), address(this), _wantAmt)"
            }
        ],
        "getReward": [
            {
                "Function": "earn",
                "Node": "IERC20(earnedAddress).safeTransfer(govAddress, fee)"
            },
            {
                "Function": "buyBack",
                "Node": "IERC20(maticAddress).safeIncreaseAllowance(apeRouterAddress, maticAmt)"
            },
            {
                "Function": "buyBack",
                "Node": "IPancakeRouter02(apeRouterAddress).swapExactTokensForTokensSupportingFeeOnTransferTokens(maticAmt, 0, path, buyBackAddress, now)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(wantAddress).safeTransfer(GrandFarmAddress, _wantAmt)"
            }
        ]
    }
}