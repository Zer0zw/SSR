{
    "Variables": {
        "User Stake Amount": [
            "Staking.amount"
        ],
        "User Stake Reward": [
            "Staking.amount",
            "Plan.apr",
            "periodicTime"
        ],
        "User Stake Time": [
            "Staking.stakeAt",
            "Staking.endstakeAt"
        ],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": [
            "stakingToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "IERC20(stakingToken).transferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "claimEarned",
                "Node": "IERC20(stakingToken).transfer(msg.sender, _earned)"
            },
            {
                "Function": "claimReferralEarnings",
                "Node": "IERC20(stakingToken).transfer(msg.sender, _ramount)"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "IERC20(stakingToken).transfer(msg.sender, tamount - _penalty)"
            },
            {
                "Function": "unstake",
                "Node": "IERC20(stakingToken).transfer(msg.sender, _earned)"
            }
        ]
    }
}