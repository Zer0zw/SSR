{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt",
            "UserInfo.rewardLockedUp"
        ],
        "User Stake Time": [
            "UserInfo.nextHarvestUntil"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "venus"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.transferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "payOrLockupPendingVenus",
                "Node": "safeVenusTransfer(msg.sender, halfRewards)"
            },
            {
                "Function": "payOrLockupPendingVenus",
                "Node": "safeVenusTransfer(msg.sender, netRewards)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), amount)"
            }
        ]
    }
}