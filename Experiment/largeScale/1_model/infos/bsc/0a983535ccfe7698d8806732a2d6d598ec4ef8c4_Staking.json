{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "time"
        ],
        "Stake Token Address": [
            "BaseToken"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount - fees)"
            },
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(msg.sender), address(feeReceiver), fees)"
            }
        ],
        "getReward": [
            {
                "Function": "claimReward",
                "Node": "rewardToken.safeTransfer(address(msg.sender), pending)"
            },
            {
                "Function": "withdraw",
                "Node": "rewardToken.safeTransfer(address(msg.sender), pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), user.amount)"
            }
        ]
    }
}