{
    "Variables": {
        "User Stake Amount": [
            "_balances"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "rewardsDuration"
        ],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": [
            "_rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "IERC20(_rewardToken).safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(msg.sender, amount)"
            }
        ]
    }
}