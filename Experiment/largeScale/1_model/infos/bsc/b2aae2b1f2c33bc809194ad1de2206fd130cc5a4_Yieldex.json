{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.shares"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt",
            "indexLpTotalShares.totalReward"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "PoolInfo.Lpaddress"
        ],
        "Reward Token Address": [
            "tenToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IERC20(tenToken).safeTransfer(msg.sender,(rewardShare.div(1e12).sub(subValue))"
            }
        ],
        "getReward": [
            {
                "Function": "returnReward",
                "Node": "IERC20(tenToken).safeTransfer(msg.sender,_rewardShare)"
            },
            {
                "Function": "withdraw",
                "Node": "IERC20(tenToken).safeTransfer(msg.sender,(rewardShare.div(1e12).sub(subValue)))"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(iAddress).safeTransfer(msg.sender,IERC20(iAddress).balanceOf(address(this)))"
            },
            {
                "Function": "withdraw",
                "Node": "IERC20(tokenAddress).safeTransfer(msg.sender,_returned1.add(_returned2))"
            },
            {
                "Function": "withdraw",
                "Node": "Address.sendValue(payable(msg.sender),_returned1.add(_returned2))"
            }
        ]
    }
}