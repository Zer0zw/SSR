{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "lpToken"
        ],
        "Reward Token Address": [
            "royal"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "lpToken[_pid].safeTransferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "settlePendingRoyal",
                "Node": "royal.transfer(_user, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "lpToken[_pid].safeTransfer(msg.sender, _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "lpToken[_pid].safeTransfer(msg.sender, amount)"
            }
        ]
    }
}