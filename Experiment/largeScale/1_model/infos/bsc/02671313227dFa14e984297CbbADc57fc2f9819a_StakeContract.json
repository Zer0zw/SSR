        "getReward": [
            {
                "Function": "withdraw",
                "Node": "token.transfer(msg.sender, amount)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "token.transfer(msg.sender, amount)"
            }
        ]
    }
}