{
    "Variables": {
        "User Stake Amount": [
            "User.staking"
        ],
        "User Stake Reward": [
            "User.pending",
            "User.extracted"
        ],
        "User Stake Time": [
            "Schedule.scheduleTime"
        ],
        "Stake Token Address": [
            "tokenCsr"
        ],
        "Reward Token Address": [
            "tokenSrc"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "provide",
                "Node": "require(IERC20(tokenCsr).transferFrom(customer, address(this), sche.scheduleAmount)"
            }
        ],
        "getReward": [
            {
                "Function": "claim",
                "Node": "require(IERC20(tokenSrc).transfer(customer, amount)"
            }
        ],
        "unStake": []
    }
}