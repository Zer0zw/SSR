{
    "Variables": {
        "User Stake Amount": [
            "_shares"
        ],
        "User Stake Reward": [
            "_rewards"
        ],
        "User Stake Time": [
            "_depositedAt"
        ],
        "Stake Token Address": [
            "stakingToken"
        ],
        "Reward Token Address": []
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "stakingToken.transferFrom(msg.sender, address(this), amount)"
            },
            {
                "Function": "depositFor",
                "Node": "stakingToken.transferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "penginChef.mintFor(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.transfer(msg.sender, amount)"
            }
        ]
    }
}