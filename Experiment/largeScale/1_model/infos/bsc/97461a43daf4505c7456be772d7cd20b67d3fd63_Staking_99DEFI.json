{
    "Variables": {
        "User Stake Amount": [
            "_balancesBUSD"
        ],
        "User Stake Reward": [
            "rewardsBUSD"
        ],
        "User Stake Time": [
            "stakingTimeBUSD"
        ],
        "Stake Token Address": [
            "BUSD"
        ],
        "Reward Token Address": [
            "BUSD"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake30Days",
                "Node": "BUSD.transferFrom(msg.sender, address(this), _amount)"
            },
            {
                "Function": "stake365Days",
                "Node": "BUSD.transferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "BUSD.transfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "totalBalanceBUSD -= _amount\n_balancesBUSD[msg.sender] -= _amount"
            }
        ]
    }
}