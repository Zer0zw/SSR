{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "_reward"
        ],
        "User Stake Time": [
            "periodFinish"
        ],
        "Stake Token Address": [
            "stakedToken"
        ],
        "Reward Token Address": []
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakedToken.transferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakedToken.transfer(address(msg.sender), user.amount)"
            }
        ]
    }
}