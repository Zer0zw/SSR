{
    "Variables": {
        "User Stake Amount": [
            "stakedTokens",
            "User.totalStakedTokenUser",
            "Stake.amount"
        ],
        "User Stake Reward": [
            "Stake.reward",
            "User.totalClaimedRewardTokenUser"
        ],
        "User Stake Time": [
            "Stake.staketime",
            "Stake.withdrawtime"
        ],
        "Stake Token Address": [
            "token"
        ],
        "Reward Token Address": [
            "token"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "token.transferFrom(msg.sender, owner, amount)"
            }
        ],
        "getReward": [
            {
                "Function": "withdraw",
                "Node": "token.transferFrom(owner, msg.sender, stakersRecord[msg.sender][index].reward)"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "token.transferFrom(msg.sender, owner, deductionAmount)"
            }
        ]
    }
}