{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "UserInfo.start"
        ],
        "Stake Token Address": [
            "PoolInfo.token"
        ],
        "Reward Token Address": [
            "rbtrToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.token.safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "deposit",
                "Node": "safeRBTRTransfer(msg.sender, pending)"
            },
            {
                "Function": "withdraw",
                "Node": "safeRBTRTransfer(msg.sender, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.token.safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.token.safeTransfer(address(msg.sender), amount)"
            }
        ]
    }
}