{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.totalEarned",
            "UserInfo.totalBonus"
        ],
        "User Stake Time": [
            "UserInfo.depositTime",
            "UserInfo.lockTime"
        ],
        "Stake Token Address": [
            "stakedToken"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "stakedToken.safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "deposit",
                "Node": "rewardToken.safeTransfer(address(msg.sender), pending)"
            },
            {
                "Function": "deposit",
                "Node": "rewardToken.safeTransfer(address(msg.sender), bonus)"
            },
            {
                "Function": "withdraw",
                "Node": "rewardToken.safeTransfer(address(msg.sender), pending)"
            },
            {
                "Function": "withdraw",
                "Node": "rewardToken.safeTransfer(address(msg.sender), bonus)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakedToken.safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "stakedToken.safeTransfer(address(msg.sender), amountToTransfer)"
            }
        ]
    }
}