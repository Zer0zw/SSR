{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "UserInfo.lastDepositBlock"
        ],
        "Stake Token Address": [
            "PoolInfo.want"
        ],
        "Reward Token Address": [
            "earningToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.want.safeTransferFrom(address(msg.sender), address(this), _wantAmt)"
            }
        ],
        "getReward": [
            {
                "Function": "deposit",
                "Node": "safeEarningsTransfer(msg.sender, pending)"
            },
            {
                "Function": "withdraw",
                "Node": "safeEarningsTransfer(msg.sender, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.want.safeTransfer(address(msg.sender), _wantAmt)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.want.safeTransfer(address(msg.sender), amount"
            }
        ]
    }
}