{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "UserInfo.timestamp"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "token"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(msg.sender, address(this), _amount"
            }
        ],
        "getReward": [
            {
                "Function": "withdrawRewards",
                "Node": "safeTokenTransfer(msg.sender, pending)"
            }
        ],
        "unStake": []
    }
}