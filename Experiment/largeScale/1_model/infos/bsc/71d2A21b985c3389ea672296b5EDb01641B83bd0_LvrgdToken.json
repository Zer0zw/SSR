{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt",
            "UserInfo.bonusDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "PoolInfo.stakeToken"
        ],
        "Reward Token Address": [
            "lvrgd"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "IERC20(pool.stakeToken).safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "safeLvrgdTransfer(_to, pending)"
            },
            {
                "Function": "_harvest",
                "Node": "safeLvrgdTransfer(_to, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC20(pool.stakeToken).safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "withdrawAll",
                "Node": "IERC20(pool.stakeToken).safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "IERC20(pool.stakeToken).safeTransfer(address(msg.sender), user.amount)"
            }
        ]
    }
}