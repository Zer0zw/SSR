{
    "Variables": {
        "User Stake Amount": [
            "_balances"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "timeLock"
        ],
        "Stake Token Address": [
            "stakeToken"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakeToken.safeTransferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardToken.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pendingWithdrawal[msg.sender] = pendingWithdrawal[msg.sender].add(amount)"
            },
            {
                "Function": "claimWithdrawal",
                "Node": "stakeToken.safeTransfer(msg.sender, amount)"
            }
        ]
    }
}