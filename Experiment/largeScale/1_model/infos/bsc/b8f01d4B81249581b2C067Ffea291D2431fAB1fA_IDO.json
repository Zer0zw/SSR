{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "stakingtime"
        ],
        "Stake Token Address": [
            "_depositToken"
        ],
        "Reward Token Address": [
            "_rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "super._stake(amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardToken.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "getReward()"
            },
            {
                "Function": "exit",
                "Node": "getReward()"
            }
        ]
    }
}