            "LpStakeInfo.amountStaked"
        ],
        "User Stake Reward": [
            "_unlocks"
        ],
        "User Stake Time": [
            "LpStakeInfo.blockNumber"
        ],
        "Stake Token Address": [
            "token"
        ],
        "Reward Token Address": []
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "_pullToken(token, _msgSender(), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "claimUnlocked",
                "Node": "_mintUnlocked(_msgSender(), unlockedAmount)"
