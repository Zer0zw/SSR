            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "super.withdraw(amount)"
            },
            {
                "Function": "exit",
                "Node": "super.withdraw(amount)"
            },
            {
                "Function": "exit",
                "Node": "sushi.safeTransfer(msg.sender, reward)"
            },
