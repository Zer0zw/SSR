{
    "Variables": {
        "User Stake Amount": [
            "stakingTAmount",
            "StakingInfo.stakingAmount"
        ],
        "User Stake Reward": [
            "referralAmount",
            "ClaimInfo.rewardAmount",
            "ReferralInfo.referralAmount"
        ],
        "User Stake Time": [
            "StakingInfo.stakingDate",
            "StakingInfo.claimDate"
        ],
        "Stake Token Address": [
            "FINEVIPAddress"
        ],
        "Reward Token Address": [
            "USDC"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "Stake",
                "Node": "ERC20(FINEVIPAddress).transferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "claimReward",
                "Node": "ERC20(USDC).transfer(msg.sender, rewardAmount)"
            }
        ],
        "unStake": [
            {
                "Function": "Unstake",
                "Node": "ERC20(FINEVIPAddress).transfer(msg.sender, _stakingInfo[msg.sender][idx].stakingAmount)"
            }
        ]
    }
}