{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "UserInfo.lastDepositTime"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "pic"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount"
            }
        ],
        "getReward": [
            {
                "Function": "withdraw",
                "Node": "safePICTransfer(msg.sender, pending.sub(punishamount))"
            },
            {
                "Function": "withdraw",
                "Node": "safePICTransfer(burnaddr, punishamount)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount.sub(vaultamount))"
            },
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(mkaddr, vaultamount)"
            },
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), user.amount"
            }
        ]
    }
}