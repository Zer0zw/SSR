            "_rewardTokens"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardToken.safeTransfer(msg.sender, rewardAmount)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(msg.sender, amount)"
            }
        ]
    }
}