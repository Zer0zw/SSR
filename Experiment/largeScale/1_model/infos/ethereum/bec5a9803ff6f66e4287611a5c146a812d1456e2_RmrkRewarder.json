{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt",
            "UserInfo.unpaidRewards"
        ],
        "User Stake Time": [
            "PoolInfo.lastRewardTime"
        ],
        "Stake Token Address": [
            "masterLpToken"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "onSushiReward",
                "Node": "user.amount = lpTokenAmount"
            }
        ],
        "getReward": [
            {
                "Function": "onSushiReward",
                "Node": "rewardToken.safeTransfer(to, pending)"
            },
            {
                "Function": "onSushiReward",
                "Node": "rewardToken.safeTransfer(to, balance)"
            }
        ],
        "unStake": []
    }
}