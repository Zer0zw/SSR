{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.reward"
        ],
        "User Stake Time": [
            "UserInfo.depositTime"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(msg.sender, address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "safeTokenTransfer(msg.sender, _reward)"
            },
            {
                "Function": "withdrawAndHarvest",
                "Node": "safeTokenTransfer(msg.sender, _reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "pool.lpToken.safeTransfer(msg.sender, _amount)"
            },
            {
                "Function": "withdrawAndHarvest",
                "Node": "pool.lpToken.safeTransfer(msg.sender, _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(msg.sender, _amount)"
            }
        ]
    }
}