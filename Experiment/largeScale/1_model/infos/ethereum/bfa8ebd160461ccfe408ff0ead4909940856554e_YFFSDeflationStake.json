{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "DURATION"
        ],
        "Stake Token Address": [
            "stakeToken"
        ],
        "Reward Token Address": [
            "stakeToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "super.stake(amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "stakeToken.safeTransfer(msg.sender, reward.sub(deflationReward))"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "super.withdraw(amount)"
            },
            {
                "Function": "exitStake",
                "Node": "withdraw(balanceOf(msg.sender))"
            }
        ]
    }
}