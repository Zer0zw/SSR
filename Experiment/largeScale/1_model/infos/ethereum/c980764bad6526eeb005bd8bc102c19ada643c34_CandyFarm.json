{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "stakedTokenPop"
        ],
        "Reward Token Address": [
            "rewardToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "stakedTokenPop.safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "deposit",
                "Node": "rewardToken.safeTransfer(address(msg.sender), pending)"
            },
            {
                "Function": "withdraw",
                "Node": "rewardToken.safeTransfer(address(msg.sender), pending)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakedTokenPop.safeTransfer(address(msg.sender), _amount)"
            }
        ]
    }
}