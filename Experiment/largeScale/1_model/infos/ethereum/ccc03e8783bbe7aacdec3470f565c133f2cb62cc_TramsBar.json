{
    "Variables": {
        "User Stake Amount": [
            "_amount"
        ],
        "User Stake Reward": [
            "what"
        ],
        "User Stake Time": [],
        "Stake Token Address": [
            "trams"
        ],
        "Reward Token Address": [
            "trams"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "enter",
                "Node": "trams.transferFrom(msg.sender, address(this), _amount"
            }
        ],
        "getReward": [],
        "unStake": [
            {
                "Function": "leave",
                "Node": "trams.transfer(msg.sender, what)"
            }
        ]
    }
}