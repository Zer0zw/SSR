{
    "Variables": {
        "User Stake Amount": [
            "_balances"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "rewardsDuration"
        ],
        "Stake Token Address": [
            "tokens"
        ],
        "Reward Token Address": [
            "rewardsToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "IERC721(stakingToken).transferFrom(msg.sender, address(this), tokenId)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "rewardsToken.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "IERC721(stakingToken).transferFrom(address(this), msg.sender, tokenId)"
            }
        ]
    }
}