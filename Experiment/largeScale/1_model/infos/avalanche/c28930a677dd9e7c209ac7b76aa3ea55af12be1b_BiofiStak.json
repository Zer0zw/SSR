{
    "Variables": {
        "User Stake Amount": [
            "Stake.utilityTokenAmount"
        ],
        "User Stake Reward": [
            "Stake.totalInterestWithdrawan"
        ],
        "User Stake Time": [
            "Stake.duration"
        ],
        "Stake Token Address": [
            "utilityTokenAddress"
        ],
        "Reward Token Address": [
            "utilityTokenAddress"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stakeTemplate",
                "Node": "require(utilityToken.transferFrom(msg.sender, address(this), utilityTokenAmount)"
            }
        ],
        "getReward": [
            {
                "Function": "closeContract",
                "Node": "require(utilityToken.transfer(staker, totalPayable)"
            }
        ],
        "unStake": [
            {
                "Function": "closeContract",
                "Node": "require(utilityToken.transfer(staker, totalPayable)"
            }
        ]
    }
}