{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.rewardDebt"
        ],
        "User Stake Time": [
            "UserInfo.nextHarvestUntil"
        ],
        "Stake Token Address": [
            "PoolInfo.lpToken"
        ],
        "Reward Token Address": [
            "swiftToken"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "payOrBurnPendingSwift",
                "Node": "safeSwiftTransfer(_user, pending.sub(devReward))"
            },
            {
                "Function": "payOrBurnPendingSwift",
                "Node": "safeSwiftTransfer(_user, pending)"
            }
        ],
        "unStake": [
            {
                "Function": "_withdraw",
                "Node": "pool.lpToken.safeTransfer(address(_user), _amount)"
            },
            {
                "Function": "emergencyWithdraw",
                "Node": "pool.lpToken.safeTransfer(address(msg.sender), amount)"
            }
        ]
    }
}