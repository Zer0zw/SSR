{
    "Variables": {
        "User Stake Amount": [
            "Claim.deposit"
        ],
        "User Stake Reward": [],
        "User Stake Time": [
            "Claim.expiry"
        ],
        "Stake Token Address": [
            "Time"
        ],
        "Reward Token Address": [
            "Memories"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "Memories.safeTransfer( address(warmupContract), _amount"
            }
        ],
        "getReward": [
            {
                "Function": "claim",
                "Node": "warmupContract.retrieve( _recipient,  amount)"
            }
        ],
        "unStake": [
            {
                "Function": "unstake",
                "Node": "Time.safeTransfer( msg.sender, _amount)"
            }
        ]
    }
}