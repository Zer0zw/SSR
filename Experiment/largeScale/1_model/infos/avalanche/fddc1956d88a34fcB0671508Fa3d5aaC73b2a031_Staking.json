{
    "Variables": {
        "User Stake Amount": [
            "UserInfo.amount"
        ],
        "User Stake Reward": [
            "UserInfo.pendingRewards"
        ],
        "User Stake Time": [
            "UserInfo.depositTime"
        ],
        "Stake Token Address": [
            "PoolInfo.stakingToken"
        ],
        "Reward Token Address": [
            "kacyAddress"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "stakingToken.safeTransferFrom(msg.sender, address(this), amount)"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "kacy.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "stakingToken.safeTransfer(msg.sender, amount)"
            }
        ]
    }
}