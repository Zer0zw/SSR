{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "starttime",
            "stoptime"
        ],
        "Stake Token Address": [
            "ethToken"
        ],
        "Reward Token Address": [
            "token"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "super.stake(amount, 0, userFreMinePool[msg.sender])"
            }
        ],
        "getReward": [
            {
                "Function": "getReward",
                "Node": "token.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "exit",
                "Node": "EthMinePool(userFreMinePool[msg.sender]).transfer(IERC20(ethToken), msg.sender, usdtBalance.sub(fee))"
            },
            {
                "Function": "exit",
                "Node": "super.withdraw(amount)"
            }
        ]
    }
}