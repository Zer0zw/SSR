{
    "Variables": {
        "User Stake Amount": [
            "userInfo.farmedToken"
        ],
        "User Stake Reward": [
            "wyzthReward",
            "uleReward"
        ],
        "User Stake Time": [
            "userInfo.lockableDays"
        ],
        "Stake Token Address": [
            "wyzthTOKEN",
            "ULETOKEN"
        ],
        "Reward Token Address": [
            "wyzthTOKEN",
            "ULETOKEN"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "farmWYZTH",
                "Node": "wyzthTOKEN.transferFrom(msg.sender, address(this), _amount)"
            },
            {
                "Function": "farmTRX",
                "Node": "user.farmedToken = msg.value"
            }
        ],
        "getReward": [
            {
                "Function": "harvest",
                "Node": "safeTransferWYZ(msg.sender, amounttoTransferred)"
            },
            {
                "Function": "harvest",
                "Node": "safeTransferULE(msg.sender, uleReward)"
            }
        ],
        "unStake": [
            {
                "Function": "harvest",
                "Node": "msg.sender.transfer(user.farmedToken)"
            },
            {
                "Function": "harvest",
                "Node": "safeTransferWYZ(msg.sender, amounttoTransferred)"
            }
        ]
    }
}