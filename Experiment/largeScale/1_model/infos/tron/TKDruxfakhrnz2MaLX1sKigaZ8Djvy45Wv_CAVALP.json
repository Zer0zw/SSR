{
    "Variables": {
        "User Stake Amount": [
            "balanceOf"
        ],
        "User Stake Reward": [
            "rewards"
        ],
        "User Stake Time": [
            "starttime",
            "finishtime"
        ],
        "Stake Token Address": [
            "token"
        ],
        "Reward Token Address": [
            "y"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "deposit",
                "Node": "super.deposit(amount)"
            }
        ],
        "getReward": [
            {
                "Function": "earned",
                "Node": "balanceOf(account).mul(rewardPerToken().sub(userRewardPerTokenPaid[account])).div(1e18).add(rewards[account])"
            }
        ],
        "unStake": [
            {
                "Function": "withdraw",
                "Node": "super.withdraw(amount)"
            },
            {
                "Function": "exit",
                "Node": "withdraw(balanceOf(msg.sender))"
            }
        ]
    }
}