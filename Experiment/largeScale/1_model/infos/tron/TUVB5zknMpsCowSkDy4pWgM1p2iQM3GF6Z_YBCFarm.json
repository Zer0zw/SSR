{
    "Variables": {
        "User Stake Amount": [
            "User.amount",
            "User.lp_amount"
        ],
        "User Stake Reward": [
            "User.stake_reward",
            "User.referral_reward"
        ],
        "User Stake Time": [
            "Order.time"
        ],
        "Stake Token Address": [
            "lpToken"
        ],
        "Reward Token Address": [
            "token"
        ]
    },
    "Calculations": {
        "Stake": [
            {
                "Function": "stake",
                "Node": "lpToken.transferFrom(_addr,address(this), _amount)"
            }
        ],
        "getReward": [
            {
                "Function": "withdraw",
                "Node": "token.safeTransfer(msg.sender, reward)"
            }
        ],
        "unStake": [
            {
                "Function": "cancelStake",
                "Node": "lpToken.safeTransfer(msg.sender, stake_amount)"
            },
            {
                "Function": "cancelStake",
                "Node": "token.safeTransfer(msg.sender, reward)"
            }
        ]
    }
}